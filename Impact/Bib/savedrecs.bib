@article{ WOS:000430505300001,
Author = {Brannon, Erin E. and Cushing, Christopher C. and Walters, Ryan W. and
   Crick, Christopher and Noser, Amy E. and Mullins, Larry L.},
Title = {Goal feedback from whom? A physical activity intervention using an
   N-of-1 RCT},
Journal = {PSYCHOLOGY \& HEALTH},
