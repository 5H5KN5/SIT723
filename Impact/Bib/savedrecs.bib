@article{ WOS:000323974300001,
Author = {Montgomery, Paul and Grant, Sean and Hopewell, Sally and Macdonald,
   Geraldine and Moher, David and Michie, Susan and Mayo-Wilson, Evan},
Title = {Protocol for CONSORT-SPI: an extension for social and psychological
   interventions},
