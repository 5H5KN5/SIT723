@article{ WOS:000290695300009,
Author = {DePadilla, Lara and Windle, Michael and Wingood, Gina and Cooper, Hannah
   and DiClemente, Ralph},
Title = {Condom Use Among Young Women: Modeling the Theory of Gender and Power},
Journal = {HEALTH PSYCHOLOGY},
