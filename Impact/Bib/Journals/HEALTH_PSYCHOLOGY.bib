@article{ WOS:000563083500009,
Author = {McHugh, R. Kathryn and Nguyen, Minh D. and Fitzmaurice, Garrett M. and
   Dillon, Daniel G.},
Title = {Behavioral Strategies to Reduce Stress Reactivity in Opioid Use
   Disorder: Study Design},
Journal = {HEALTH PSYCHOLOGY},
