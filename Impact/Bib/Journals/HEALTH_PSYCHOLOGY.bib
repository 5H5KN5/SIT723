@article{ WOS:000497755800013,
Author = {McVay, Megan A. and Bennett, Gary G. and Steinberg, Dori and Voils,
   Corrine I.},
Title = {Dose-Response Research in Digital Health Interventions: Concepts,
   Considerations, and Challenges},
Journal = {HEALTH PSYCHOLOGY},
