@article{ WOS:000318523400015,
Author = {Mermelstein, Robin J. and Revenson, Tracey A.},
Title = {Applying Theory Across Settings, Behaviors, and Populations:
   Translational Challenges and Opportunities},
Journal = {HEALTH PSYCHOLOGY},
Year = {2013},
Volume = {32},
Number = {5, SI},
Pages = {592-596},
Month = {MAY},
Abstract = {Basic social psychological theories have much to contribute to our
   understanding of health problems and health-related behaviors and may
   provide potential avenues for intervention development. However, for
   these theories to have broader reach and applicability to the field of
   health psychology, more work needs to be done in integrating contexts
   into these theories and addressing more specifically their application
   across settings, behaviors, and populations. We argue that integration
   of these theories into a broader multidisciplinary and multilevel
   ecological framework is needed to enhance their translation into
   real-world applications. To enhance this translation, we make several
   recommendations, including breaking down silos between disciplinary
   perspectives and enhancing bidirectional communication and translation;
   analyzing boundary conditions of theories; expanding research approaches
   to move outside the laboratory and maintain a focus on external
   validity; and conducting efficacy testing of theories with meaningful,
   relevant endpoints.},
