@article{ WOS:000472220600008,
Author = {Liebmann, Edward P. and Preacher, Kristopher J. and Richter, Kimber P.
   and Cupertino, A. Paula and Catley, Delwyn},
Title = {Identifying Pathways to Quitting Smoking via Telemedicine-Delivered Care},
Journal = {HEALTH PSYCHOLOGY},
