Address = {CAMPUS, 4 CRINAN ST, LONDON N1 9XW, ENGLAND},
Type = {Article},
Language = {English},
Affiliation = {de Luca, K (Corresponding Author), Macquarie Univ, Fac Med Hlth \& Human Sci, Dept Chiropract, Sydney, NSW, Australia.
   de Luca, K (Corresponding Author), CQ Univ, Sch Hlth Med \& Appl Sci, Discipline Chiropract, Brisbane, Qld, Australia.
