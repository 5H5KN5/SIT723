Title = {Training for mental health professionals in responding to experienced
   and anticipated mental health-related discrimination (READ-MH): protocol
   for an international multisite feasibility study},
Journal = {PILOT AND FEASIBILITY STUDIES},
Year = {2022},
Volume = {8},
