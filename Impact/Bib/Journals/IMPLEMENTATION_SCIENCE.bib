@article{ WOS:000650621800001,
Author = {Faija, Cintia L. and Gellatly, Judith and Barkham, Michael and Lovell,
   Karina and Rushton, Kelly and Welsh, Charlotte and Brooks, Helen and
   Ardern, Kerry and Bee, Penny and Armitage, Christopher J.},
Title = {Enhancing the Behaviour Change Wheel with synthesis, stakeholder
