	volume = {55},
	issn = {2373-9231},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/pra2.2018.14505501143},
	doi = {10.1002/pra2.2018.14505501143},
	abstract = {The topic modeling approach can indicate hidden relationships between articles in a particular academic discipline. This study aims to examine topics in library and information science ({LIS}) using the latent Dirichlet allocation method. From representative five journals, 1,648 full-text articles were analyzed. We labeled 30 identified topics based on the top 10 highly weighted terms for each topic, title, and body of articles. From the topic mapping, commonly used methods and shift of research issues in {LIS} were found.},
	pages = {847--848},
	number = {1},
	journaltitle = {Proceedings of the Association for Information Science and Technology},
	author = {Kurata, Keiko and Miyata, Yosuke and Ishita, Emi and Yamamoto, Michimasa and Yang, Fang and Iwase, Azusa},
	urldate = {2023-12-22},
	date = {2018},
	langid = {english},
	note = {\_eprint: https://asistdl.onlinelibrary.wiley.com/doi/pdf/10.1002/pra2.2018.14505501143},
	keywords = {latent Dirichlet allocation, library and information science research, mapping, Topic modeling},
	file = {Full Text PDF:C\:\\Users\\hrsto\\Zotero\\storage\\ZCZSD8QS\\Kurata et al. - 2018 - Analyzing library and information science full-tex.pdf:application/pdf;Snapshot:C\:\\Users\\hrsto\\Zotero\\storage\\MM2HMVTN\\pra2.2018.html:text/html},
}

@article{grbin_development_2022,
	title = {The Development of a Living Knowledge System and Implications for Future Systematic Searching},
	volume = {71},
	issn = {2475-0158},
	url = {https://doi.org/10.1080/24750158.2022.2087954},
	doi = {10.1080/24750158.2022.2087954},
	abstract = {This article details librarian roles in applying traditional information knowledge and skills while co-developing a Living Knowledge Literature Review System ({LKS}). Increasing literature volumes and timelines for evidence synthesis risk industry decisions being informed by outdated data. Living reviews and synthesis challenge these models but include processes which can be streamlined. The {LKS} semi-automates literature review processes in particular knowledge domains using a novel machine learning algorithm. The {LKS} aims to distinguish relevant from irrelevant search results from conventional library searches, delivering outputs to researchers for summarisation and dissemination via a community targeted, purpose-built living evidence online portal ({SEED} {POD}). {LKS} screening accuracy is continuously improved through researcher and end-user feedback to create a more precise, semi-automated, living literature review system. Working closely with development teams and researchers in Social Early Emotional Development ({SEED}), librarians applied traditional information and searching knowledge into a new automation context, assisting in research question refinement, literature search development and licencing advice to build and optimise {LKS} functioning. Librarian input was highly valued in {LKS} production and optimisation. Including librarians in projects concerning automated evidence synthesis can enhance outcomes and lead to continued consultation and collaboration.},
	pages = {275--292},
	number = {3},
	journaltitle = {Journal of the Australian Library and Information Association},
	author = {Grbin, Lisa and Nichols, Peter and Russell, Fiona and Fuller-Tyszkiewicz, Matthew and Olsson, Craig A.},
	urldate = {2024-02-03},
	date = {2022-07-03},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/24750158.2022.2087954},
	keywords = {systematic review, academic libraries, artificial intelligence, living review, machine learning, Semi-automation},
}

