@article{michie_making_2005,
	title = {Making psychological theory useful for implementing evidence based practice: a consensus approach},
	volume = {14},
	issn = {1475-3898, 1475-3901},
	url = {https://qualitysafety.bmj.com/lookup/doi/10.1136/qshc.2004.011155},
